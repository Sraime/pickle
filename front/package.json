{
  "name": "front_module",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:dev",
    "start:dev": "ng serve --host 0.0.0.0",
    "start:test": "ng serve --host 0.0.0.0 --configuration=test --port 4201",
    "build": "ng build --prod",
    "test:unit": "ng test --coverage",
    "test:unit:clearCache": "ng test --coverage --clearCache",
    "test:debug": "node --inspect node_modules/.bin/jest src/app/components/amazing --config=./jest.config.js --runInBand",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.4",
    "@angular/cdk": "^10.0.2",
    "@angular/common": "^10.0.4",
    "@angular/compiler": "^10.0.4",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^10.0.4",
    "@angular/material": "^10.0.2",
    "@angular/platform-browser": "^10.0.4",
    "@angular/platform-browser-dynamic": "^10.0.4",
    "@angular/router": "^10.0.4",
    "core-js": "^3.6.5",
    "file-saver": "^2.0.2",
    "rxjs": "^6.6.0",
    "socket.io-client": "^2.3.0",
    "tslib": "^2.0.0",
    "uuid": "^8.2.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^9.0.1",
    "@angular-devkit/build-angular": "^0.1000.3",
    "@angular/cli": "^10.0.3",
    "@angular/compiler-cli": "^10.0.4",
    "@angular/language-service": "^10.0.4",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.1.0",
    "jest-create-mock-instance": "^1.1.0",
    "jest-preset-angular": "^8.2.1",
    "jest-sonar-reporter": "^2.0.0",
    "json-server": "^0.16.1",
    "ng-bullet": "^1.0.3",
    "ng-mocks": "^10.0.2",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-jest": "^1.0.5",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.9.7"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "ut_report.xml",
    "indent": 4,
    "sonar56x": true
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/test-setup.ts"
    ]
  }
}
