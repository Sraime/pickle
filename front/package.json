{
	"name": "front_module",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "npm run start:dev",
		"start:dev": "ng serve --host 0.0.0.0",
		"start:test": "ng serve --host 0.0.0.0 --configuration=test --port 4201",
		"build": "ng build",
		"test:unit": "ng test --coverage",
		"test:debug": "node --inspect node_modules/.bin/jest src/app/components/amazing --config=./jest.config.js --runInBand",
		"lint": "ng lint",
		"lint:fix": "ng lint --fix",
		"e2e": "ng e2e"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^9.0.6",
		"@angular/cdk": "^9.1.2",
		"@angular/common": "^9.0.6",
		"@angular/compiler": "^9.0.6",
		"@angular/core": "^9.0.6",
		"@angular/forms": "^9.0.6",
		"@angular/material": "^9.1.2",
		"@angular/platform-browser": "^9.0.6",
		"@angular/platform-browser-dynamic": "^9.0.6",
		"@angular/router": "^9.0.6",
		"core-js": "^2.6.11",
		"file-saver": "^2.0.2",
		"rxjs": "^6.5.4",
		"socket.io-client": "^2.3.0",
		"tslib": "^1.11.1",
		"uuid": "^3.4.0",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-builders/jest": "^8.3.2",
		"@angular-devkit/build-angular": "^0.900.5",
		"@angular/cli": "^9.0.5",
		"@angular/compiler-cli": "^9.0.6",
		"@angular/language-service": "^9.0.6",
		"@types/jasmine": "~2.8.8",
		"@types/jasminewd2": "^2.0.8",
		"@types/jest": "^24.9.1",
		"@types/node": "~8.9.4",
		"codelyzer": "^5.2.1",
		"concurrently": "^4.1.2",
		"jasmine-core": "~2.99.1",
		"jasmine-spec-reporter": "~4.2.1",
		"jest": "^24.9.0",
		"jest-create-mock-instance": "^1.1.0",
		"jest-preset-angular": "^7.1.1",
		"jest-sonar-reporter": "^2.0.0",
		"json-server": "^0.14.2",
		"ng-bullet": "^1.0.3",
		"ng-mocks": "^9.0.0",
		"prettier": "^1.19.1",
		"ts-node": "~7.0.0",
		"tslint": "~5.11.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-jest": "^1.0.5",
		"tslint-plugin-prettier": "^2.1.0",
		"typescript": "~3.7.5"
	},
	"jestSonar": {
		"reportPath": "reports",
		"reportFile": "ut_report.xml",
		"indent": 4,
		"sonar56x": true
	},
	"jest": {
		"preset": "jest-preset-angular",
		"setupFilesAfterEnv": [
			"<rootDir>/src/test-setup.ts"
		]
	}
}
